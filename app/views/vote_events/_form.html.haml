= form_for @vote_event do |f|
  - if @vote_event.errors.any?
    #error_explanation
      %h2= "#{pluralize(@vote_event.errors.count, "error")} prohibited this vote_event from being saved:"
      %ul
        - @vote_event.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :start_date
    = f.datetime_select :start_date
  .field
    = f.label :end_date
    = f.datetime_select :end_date
  .actions
    = f.submit 'Save'
